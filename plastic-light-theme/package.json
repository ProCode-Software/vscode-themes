{
  "name": "plastic-light",
  "displayName": "Plastic Light",
  "version": "1.0.0",
  "private": true,
  "description": "A light version of the Plastic theme by Will Stone",
  "categories": [
    "Themes"
  ],
  "homepage": "https://github.com/ProCode-Software/Plastic-Light",
  "bugs": {
    "url": "https://github.com/ProCode-Software/Plastic-Light"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ProCode-Software/Plastic-Light"
  },
  "author": "ProCode Software",
  "publisher": "procode-software",
  "type": "module",
  "scripts": {
    "build-theme": "tsx ./src/index.ts",
    "prerelease": "npm run generate-theme"
