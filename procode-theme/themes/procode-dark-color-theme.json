    "tokenColors": [
        {
            "scope": [
                "punctuation",
                "meta.brace",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#a3afbe"
            }
        },
        {
            "scope": [
                "support.type.primitive",
                "support.type.builtin",
                "source.go storage.type"
            ],
            "settings": {
                "foreground": "#22c3c8"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.name.type.class",
