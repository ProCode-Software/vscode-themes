	"tokenColors": [
		{
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "--t-comment"
			}
		},
		{
			"scope": [
				"constant",
				"entity.name.constant",
				"variable.other.constant",
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "--t-const"
			}
		},
		{
			"scope": ["support.constant"],
			"settings": {
				"foreground": "--t-bool"
			}
		},
		{
			"scope": ["constant.other.character-class.range.regexp"],
			"settings": {
				"foreground": "--t-class"
			}
		},
		{
			"scope": [
				"constant.character.character-class.regexp",
				"constant.other.character-class.regexp"
			],
			"settings": {
				"foreground": "--t-class"
			}
		},
		{
			"scope": ["constant.character.entity"],
			"settings": {
				"foreground": "--t-escape"
			}
		},
		{
			"scope": [
				"constant.character.escape",
				"constant.character",
				"constant.other.placeholder"
			],
			"settings": {
				"foreground": "--t-escape"
			}
		},
		{
			"scope": ["constant.language.boolean", "constant.language"],
			"settings": {
				"foreground": "--t-bool"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent",
				"meta.preprocessor.numeric"
			],
			"settings": {
				"foreground": "--t-bool"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"support.constant.handlebars",
				"entity.name.operator.custom-literal"
			],
			"settings": {
				"foreground": "--t-function"
			}
		},
		{
			"scope": [
				"keyword.other.doctype",
				"meta.tag.metadata.doctype entity.name.tag",
				"meta.tag.metadata.doctype entity.name.tag.html"
			],
			"settings": {
				"foreground": "--t-type"
			}
		},
		{
			"scope": ["entity.name.tag"],
			"settings": {
				"foreground": "--t-function"
			}
		},
		{
			"scope": [
				"entity.name.tag.css",
				"entity.name.tag.less",
				"entity.name.tag.html"
			],
			"settings": {
				"foreground": "--t-function"
			}
		},
		{
			"scope": [
				"entity.name.tag.reference.scss",
				"entity.name.tag.wildcard"
			],
			"settings": {
				"foreground": "--t-keyword"
			}
		},
		{
			"scope": ["entity.name.tag.yaml"],
			"settings": {
				"foreground": "--t-prop"
			}
		},
		{
			"scope": [
				"entity.name.type",
				"entity.name.type.class",
				"support.class",
				"entity.name.type.interface",
				"variable.defaultLibrary",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "--t-class"
			}
		},
		{
			"scope": ["entity.name.type.instance.jsdoc"],
			"settings": {
				"foreground": "--t-class"
			}
		},
		{
			"scope": ["entity.name.type.module"],
			"settings": {
				"foreground": "--t-class"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "--t-attribute"
			}
		},
		{
			"scope": ["entity.name.namespace", "entity.name.type.module"],
			"settings": {
				"foreground": "--t-namespace"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name punctuation",
				"source.css entity.other.attribute-name.class",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.parent.less",
				"entity.other.attribute-name.scss",
				"punctuation.definition.constant.css"
			],
			"settings": {
				"foreground": "--t-class"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.id punctuation",
				"source.css.less entity.other.attribute-name.id"
			],
			"settings": {
				"foreground": "--t-string"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.pseudo-element.css",
				"entity.other.attribute-name.pseudo-element punctuation",
				"entity.other.attribute-name.pseudo-element.css"
			],
			"settings": {
				"foreground": "--t-function"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"fontStyle": "italic",
				"foreground": "--c-red"
			}
		},
		{
			"scope": [
				"keyword.control",
				"keyword",
				"keyword.other.operator",
				"variable.language.this",
				"entity.name.operator",
				"punctuation.definition.block.tag.jsdoc",
				"storage",
				"storage.modifier",
				"storage.type",
				"keyword.operator.logical.python",
				"variable.language.special",
				"punctuation.definition.keyword",
